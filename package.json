	"devDependencies": {
		"@remix-run/dev": "^2.0.1",
		"@remix-run/eslint-config": "^2.0.1",
		"@types/react": "^18.2.20",
		"@types/react-dom": "^18.2.7",
		"autoprefixer": "^10.4.16",
		"better-sqlite3": "^8.7.0",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.38.0",
		"prettier": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.5",
		"tailwindcss": "^3.3.3",
		"tsx": "^3.13.0",
		"typescript": "^5.1.6"
	},
	"engines": {
		"node": ">=18.0.0"
