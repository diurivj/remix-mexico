{
	"name": "remix-mexico",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix build",
		"dev": "remix dev --manual",
		"start": "remix-serve ./build/index.js",
		"typecheck": "tsc",
		"db:generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./drizzle/schema.ts",
		"db:migrate": "tsx drizzle/migrate",
		"db:studio": "drizzle-kit studio --port 3333"
	},
	"dependencies": {
		"@conform-to/react": "^0.9.0",
		"@conform-to/zod": "^0.9.0",
		"@libsql/client": "^0.3.5",
		"@radix-ui/react-dropdown-menu": "^2.0.6",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-label": "^2.0.2",
		"@radix-ui/react-slot": "^1.0.2",
		"@remix-run/css-bundle": "^2.0.1",
		"@remix-run/node": "^2.0.1",
		"@remix-run/react": "^2.0.1",
		"@remix-run/serve": "^2.0.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"drizzle-orm": "^0.28.6",
		"drizzle-zod": "^0.5.1",
		"isbot": "^3.6.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwind-merge": "^1.14.0",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.22.4"
